<Project>
	<PropertyGroup>

		<!-- 默认 Release 编译 -->
		<Configuration Condition="'$(Configuration)' == ''">Release</Configuration>

		<!-- 自动在 build 时生成 NuGet 包 -->
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        
		<!-- 统一 NuGet 元数据 -->
		<Authors>南京佑妍科技技术有限公司</Authors>
		<Company>南京佑妍科技技术有限公司</Company>
		
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<RepositoryUrl>https://github.com/Snsaiu/TuDog</RepositoryUrl>
		<PackageProjectUrl>https://github.com/Snsaiu/TuDog</PackageProjectUrl>

		<!-- 版本号 -->
		<Version>0.0.0.3</Version>
		<FileVersion>0.0.0.3</FileVersion>
		<AssemblyVersion>0.0.0.3</AssemblyVersion>
	</PropertyGroup>
</Project>