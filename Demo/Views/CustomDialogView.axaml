<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:viewModels="clr-namespace:Demo.ViewModels"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:DataType="{x:Type viewModels:CustomDialogViewModel}"
             x:Class="Demo.Views.CustomDialogView">
    <StackPanel>
        <Button Content="自定义无返回值弹框" Command="{Binding NoReturnShowCommand}"></Button>
        <Button Content="自定义弹框" Command="{Binding ShowCommand}"></Button>
    </StackPanel>
</UserControl>
